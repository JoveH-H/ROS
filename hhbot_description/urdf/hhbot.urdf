<?xml version="1.0"?>  
<robot name="hhbot">
    <!--base_link-->
    <link name="base_link">  
        <!--base_link_visual-->
        <visual>  
            <geometry>  
                <box size="0.25 0.14 0.05"/>  
            </geometry>  
            <origin rpy="0 0 0" xyz="0 0 0"/>  
            <material name="white">  
                <color rgba="0.96 0.96 0.98 1"/>  
            </material>  
        </visual>  
        <!--base_link_collision-->
        <collision>
           <origin xyz=" 0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.25 0.14 0.05"/>
            </geometry>
        </collision>
        <!--base_link_inertial-->
        <inertial>
            <mass value="3" />
            <inertia ixx="0.1" ixy="0.0" ixz="0.0"
                     iyy="0.1" iyz="0.0" izz="0.1" />
        </inertial>
    </link>  
 
    <link name="left_front_wheel">  
        <visual>  
            <geometry>  
                <cylinder length="0.02" radius="0.025"/>  
            </geometry>  
            <material name="black">  
                <color rgba="0.29 0.29 0.29 1"/>  
            </material>  
        </visual>  
    </link>  
 
    <joint name="left_front_wheel_joint" type="continuous">  
        <axis xyz="0 0 1"/>  
        <parent link="base_link"/>  
        <child link="left_front_wheel"/>  
        <origin rpy="1.57075 0 0" xyz="0.1 -0.07 -0.02"/>  
        <limit effort="100" velocity="100"/>  
        <joint_properties damping="0.0" friction="0.0"/>  
    </joint>  
 
    <link name="left_back_wheel">  
        <visual>  
            <geometry>  
                <cylinder length="0.02" radius="0.025"/>  
            </geometry>  
            <material name="black">  
                <color rgba="0.29 0.29 0.29 1"/>  
            </material>  
        </visual>  
    </link>  
 
    <joint name="left_back_wheel_joint" type="continuous">  
        <axis xyz="0 0 1"/>  
        <parent link="base_link"/>  
        <child link="left_back_wheel"/>  
        <origin rpy="1.57075 0 0" xyz="-0.1 -0.07 -0.02"/>  
        <limit effort="100" velocity="100"/>  
        <joint_properties damping="0.0" friction="0.0"/>  
    </joint>
  
    <link name="right_front_wheel">  
        <visual>  
            <geometry>  
                <cylinder length="0.02" radius="0.025"/>  
            </geometry>  
            <material name="black">  
                <color rgba="0.29 0.29 0.29 1"/>  
            </material>  
        </visual>  
    </link>  
 
    <joint name="right_front_wheel_joint" type="continuous">  
        <axis xyz="0 0 1"/>  
        <parent link="base_link"/>  
        <child link="right_front_wheel"/>  
        <origin rpy="1.57075 0 0" xyz="0.1 0.07 -0.02"/>  
        <limit effort="100" velocity="100"/>  
        <joint_properties damping="0.0" friction="0.0"/>  
    </joint>  
 
    <link name="right_back_wheel">  
        <visual>  
            <geometry>  
                <cylinder length="0.02" radius="0.025"/>  
            </geometry>  
            <material name="black">  
                <color rgba="0.29 0.29 0.29 1"/>  
            </material>   
        </visual>  
    </link>  
 
    <joint name="right_back_wheel_joint" type="continuous">  
        <axis xyz="0 0 1"/>  
        <parent link="base_link"/>  
        <child link="right_back_wheel"/>  
        <origin rpy="1.57075 0 0" xyz="-0.1 0.07 -0.02"/>  
        <limit effort="100" velocity="100"/>  
        <joint_properties damping="0.0" friction="0.0"/>  
    </joint>  
 
    <link name="tail">  
        <visual>  
            <geometry>  
                <box size="0.02 0.1 0.02"/>  
            </geometry>  
            <material name="red">  
                <color rgba="1 0 0 1"/>  
            </material>  
        </visual>  
    </link>

    <joint name="tail_joint" type="fixed">  
        <parent link="base_link"/>  
        <child link="tail"/>  
        <origin rpy="0 -1.0472 0" xyz=" -0.09 0 0.023"/>  
    </joint> 

</robot>

